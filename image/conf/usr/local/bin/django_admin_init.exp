#!/usr/bin/env expect

set timeout -1
spawn django-admin.py migrate --settings=graphite.settings --run-syncdb

expect "Would you like to create one now" {
  send "yes\r"
}

expect "Username" {
  send "root\r"
}

expect "Email address:" {
  send "root.graphite@mailinator.com\r"
}

expect "Password:" {
  send "root\r"
}

expect "Password *:" {
  send "root\r"
}

expect "Superuser created successfully"
